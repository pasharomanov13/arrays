#include <math.h>
#include <iostream>
#define N 10
using namespace std;

int main()
{
    int n=3;
    int k = N-n+1 ;
    int A[N];
    int res[8];
    int Sum = 0;
 //   int j=0;
    for (int a=0; a<N; a++)
    {
    cin >> A[a] ;

    }

    for (int i =0; i< N-1; i++){
       // int S;
      //  S=i+j;
	//	res[i]=0;
		for (int j=0;j<n-1;j++) { Sum+= A[i+j];}

        res[i]= Sum/n;
		Sum=0;
        //j++;
    }
    for (int b=0; b<k; b++)
    {
    cout << "result" << res[b] << endl;

    }
	int yr;
	cin >> yr;
        return 0;
}
